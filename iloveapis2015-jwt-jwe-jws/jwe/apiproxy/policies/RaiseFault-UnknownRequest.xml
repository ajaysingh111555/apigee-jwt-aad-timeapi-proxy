<RaiseFault name='RaiseFault-UnknownRequest'>
  <DisplayName>RaiseFault-UnknownRequest</DisplayName>
  <Description>Unknown Request</Description>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <FaultResponse>
    <Set>
      <Payload contentType='application/json'
               variablePrefix='%' variableSuffix='#'><![CDATA[{
  "error" : "that request was unknown",
  "message" : "try one of the following. See the readme for details.",
  "suggestions" : [
     "POST /jwe/create-hs256"
  ]
}
]]></Payload>
      <StatusCode>404</StatusCode>
      <ReasonPhrase>Not Found</ReasonPhrase>
    </Set>
  </FaultResponse>
</RaiseFault>
